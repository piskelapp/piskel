{
  "env": {
    "es6": true,
    "browser": true
  },
  "globals": {
    "$": true,
    "jQuery": true,
    "pskl": true,
    "Events": true,
    "Constants": true,
    "console": true,
    "module": true,
    "require": true,
    "Q": true,
    "Promise": true
  },
  "rules": {
    "no-undef": 2,
    "no-use-before-define": [
      2,
      {
        "functions": false
      }
    ],
    "curly": [
      2,
      "all"
    ],
    "operator-linebreak": [
      2,
      "after"
    ],
    "camelcase": [
      2,
      {
        "properties": "never"
      }
    ],
    "max-len": [
      2,
      120
    ],
    "indent": [
      2,
      2,
      {
        "SwitchCase": 1
      }
    ],
    "quotes": [
      2,
      "single"
    ],
    "no-multi-str": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-trailing-spaces": 2,
    "space-unary-ops": [
      2,
      {
        "nonwords": false,
        "overrides": {}
      }
    ],
    "one-var": [
      2,
      "never"
    ],
    "brace-style": [
      2,
      "1tbs",
      {
        "allowSingleLine": true
      }
    ],
    "keyword-spacing": [
      2,
      {}
    ],
    "space-infix-ops": 2,
    "space-before-blocks": [
      2,
      "always"
    ],
    "eol-last": 2,
    "space-in-parens": [
      2,
      "never"
    ],
    "no-multiple-empty-lines": 2,
    "no-with": 2,
    "no-spaced-func": 2,
    "dot-notation": 2,
    "semi": [
      2,
      "always"
    ]
  }
}
